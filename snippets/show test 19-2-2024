
let scale = "<C:minor F:minor>/4"

stack(
  n("0 2 <3 7> 4*2 [8 <9 12*3> 8 9]").scale(scale).midi(0),
  
  n("0*6")
  .scale(scale).midichan("<3 4>/2").midi(1),
  
  n("10 12 [9 18] 14").add("<0 7>/4")
  .scale(scale).midichan(6).midi(1),

  n("0 ~!3 5!2").scale(scale).midichan(1).midi(1),

  n("~ <~ 5> [12*4 20*2]").scale(scale).midichan(3).midi(1),
  
)
  // .segment(64)
